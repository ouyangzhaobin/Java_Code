class P_88_MergeSortedArray{
    public static void main(String[] args) {
        Solution solution = new P_88_MergeSortedArray().new Solution();
        Scanner scanner = new Scanner(System.in);
        int m = scanner.nextInt();
        int n = scanner.nextInt();
        int[] nums1 = new int[m+n];
        for (int i = 0; i < m+n; i++) {
            nums1[i] = scanner.nextInt();
        }

        int[] nums2 = new int[n];
        for (int i = 0; i < n; i++) {
            nums2[i] = scanner.nextInt();
        }

        solution.merge(nums1,m,nums2,n);

    }

//leetcode submit region begin(Prohibit modification and deletion)
class Solution {
    public void merge(int[] nums1, int m, int[] nums2, int n) {
        // æ—¶é—´å¤æ‚åº¦ä¸ºm+n
        int ans1 = 0, ans2 = m+n;
        for (int i = m-1; i >= 0; i--) {
            if(n==0){
                break;
            }
            if(nums1[i] > nums2[n-1]){
                nums1[ans2-1-ans1] = nums1[i];
            }else{
                nums1[ans2-1-ans1] = nums2[n-1];
                n--;
                i++;
            }
            ans1+=1;
        }
        if(n!=0){
            for (int i = 0; i < n; i++) {
                nums1[i] = nums2[i];
            }
        }

        /*
        // æ—¶é—´å¤æ‚åº¦ä¸ºnlogn
        if(nums1 != null){
            for (int i = m; i < m+n ; i++) {
                nums1[i] = nums2[i-m];
            }
        }
        // sortçš„å¹³å‡æ—¶é—´å¤æ‚åº¦ä¸ºOï¼ˆnlogn)
        Arrays.sort(nums1);
         */


    }
}
//leetcode submit region end(Prohibit modification and deletion)

}
